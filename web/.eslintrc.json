{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-console": 1,
    "react/destructuring-assignment": 0,
    "arrow-body-style": "off",
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}